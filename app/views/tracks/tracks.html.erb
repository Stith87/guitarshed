    <div class="container-full">       
       <div class="row tracks">
          <div class="col-md-12 col-sm-12 col-xs-12 form-container">
            <%= render 'form' %> 
          </div>
      </div>
      <div class="row">
          <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog">
              <div class="modal-content">
                <div class="modal-header">
                  <span class='close' data-dismiss='modal' onclick='close()'>X</span>
                </div>
                  
                
                <div id="track-player" class="modal-body">
                  
                </div>
              </div>
              
            </div>
          </div>

          <table class="col-md-12 col-sm-12 col-xs-12 track-list"> 
             
            <% if (defined?(@tracks)).nil? %>

            <% else %>
              <% @tracks.each do |track| %>


                <tr>
                <td><%= track.artist %></td>
                <td><%= track.title %></td>
                <td><span class="play" data-toggle="modal" data-target="#myModal" onclick="tsearch('<%= track.url%>','<%= track.artist %>','<%= track.title %>')">Play</span></td>
                </tr>


              <% end %>
            <% end %>
          </table>
        </div>
		  </div>
	  

